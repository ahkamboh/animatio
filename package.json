{
  "name": "animatio",
  "version": "1.1.2",
  "description": "A React component package for creating text effects. Customize your text animation effortlessly with customizable classNames for styling.",
  "main": "dist/cli.js",
  "types": "dist/cli.d.ts",
  "bin": {
    "animatio": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc && cpx \"src/templates/**/*\" dist/templates",
    "start": "node dist/cli.js",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist/",
    "dist/templates/"
  ],
  "keywords": [
    "animation",
    "react",
    "nextjs",
    "cli",
    "text-animations",
    "typescript"
  ],
  "author": "Ali Hamza Kamboh",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ahkamboh/animatio"
  },
  "homepage": "https://github.com/ahkamboh/animatio#readme",
  "dependencies": {
    "animejs": "^3.2.1",
    "commander": "^8.3.0",
    "@types/animejs": "^3.1.12"
  },
  "devDependencies": {
    "@types/node": "^16.11.12",
    "@types/react": "^17.0.37",
    "copyfiles": "^2.4.1",
    "cpx": "^1.5.0",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  }
}